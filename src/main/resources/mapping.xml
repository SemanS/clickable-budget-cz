                      xmlns:ctm="urn:cz:isvs:micr:schemas:CommonTypes:v1"
                      xmlns:bus="urn:cz:isvs:micr:schemas:BusinessTypes:v2"
                      xmlns:stm="urn:cz:isvs:micr:schemas:StatementTypes:v1"
                      xmlns:acc="urn:cz:isvs:micr:schemas:AccountTypes:v1"
                      xmlns:sig="http://www.w3.org/2000/09/xmldsig#"
                      xmlns="urn:cz:isvs:micr:schemas:Fin_2_12_m:v1"/>
        </field>

        <field name="outcomes" collection="arraylist" type="com.webinson.clickablebudget.dto.VykazRadekDto"
               container="false">
            <bind-xml name="VydajeRozpoctove" xmlns:cct="urn:cz:isvs:micr:schemas:CoreComponentTypes:v1"
                      xmlns:ctm="urn:cz:isvs:micr:schemas:CommonTypes:v1"
                      xmlns:bus="urn:cz:isvs:micr:schemas:BusinessTypes:v2"
                      xmlns:stm="urn:cz:isvs:micr:schemas:StatementTypes:v1"
                      xmlns:acc="urn:cz:isvs:micr:schemas:AccountTypes:v1"
                      xmlns:sig="http://www.w3.org/2000/09/xmldsig#"
                      xmlns="urn:cz:isvs:micr:schemas:Fin_2_12_m:v1"/>
        </field>

        <field name="vykazHlavicka" type="com.webinson.clickablebudget.dto.VykazHlavickaDto" container="false">
            <bind-xml name="VykazHlavicka"
                      xmlns:cct="urn:cz:isvs:micr:schemas:CoreComponentTypes:v1"
                      xmlns:ctm="urn:cz:isvs:micr:schemas:CommonTypes:v1"
                      xmlns:bus="urn:cz:isvs:micr:schemas:BusinessTypes:v2"
                      xmlns:stm="urn:cz:isvs:micr:schemas:StatementTypes:v1"
                      xmlns:acc="urn:cz:isvs:micr:schemas:AccountTypes:v1"
                      xmlns:sig="http://www.w3.org/2000/09/xmldsig#"
                      xmlns="urn:cz:isvs:micr:schemas:Fin_2_12_m:v1"/>
        </field>
    </class>

    <class name="com.webinson.clickablebudget.dto.VykazHlavickaDto">
        <map-to xml="VykazHlavicka"/>

        <field name="subjektIco" type="java.lang.Integer">
